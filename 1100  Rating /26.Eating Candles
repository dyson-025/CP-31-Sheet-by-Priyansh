Problem link: https://codeforces.com/problemset/problem/1669/F
My mistakes: Syntax error
Mistake: 

#include <bits/stdc++.h>
using namespace std;

int main() {
    int t;
    cin>>t;
    while(t--){
        int n;
        cin>>n;
        vector<int> v(n);
        for(int i=0;i<n;i++) cin>>v[i];
        vector<int> pf(n);
        pf[0]=v[0];
        for(int i=1;i<n;i++) pf[i]+=v[i];
        vector<int> sf(n);
        sf[n-1]=v[n-1];
        for(int i=n-2;i>=0;i--) sf[i]+=v[i];
        int left=0;
        int right=n-1;
        int ans=0;
        while(left<=right){
            if(pf[left]<sf[right]) left++;
            else if(pf[left]>sf[right]) right++;
            else {
               int cnt=(n-right)+(left+1);
               ans=max(ans,cnt);
               left++;
            }
        }
        cout<<ans<<endl;
    }
}
corrected code:

#include <bits/stdc++.h>
using namespace std;

int main() {
    int t;
    cin>>t;
    while(t--){
        int n;
        cin>>n;
        vector<int> v(n);
        for(int i=0;i<n;i++) cin>>v[i];
        vector<long long> pf(n);
        pf[0]=v[0];
        for(int i=1;i<n;i++) pf[i]=v[i]+pf[i-1];
        vector<long long> sf(n);
        sf[n-1]=v[n-1];
        for(int i=n-2;i>=0;i--) sf[i]=v[i]+sf[i+1];
        int left=0;
        int right=n-1;
        int ans=0;
        while(left<right){
            if(pf[left]<sf[right]) left++;
            else if(pf[left]>sf[right]) right--;
            else {
               int cnt=(n-right)+(left+1);
               ans=max(ans,cnt);
               left++;
            }
        }
        cout<<ans<<endl;
    }
}
